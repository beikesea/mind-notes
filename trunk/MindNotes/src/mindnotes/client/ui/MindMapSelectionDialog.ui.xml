<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.dialog-content {
			margin: 18px;
		}
		.buttonbar {
			text-align: right;
		}
	</ui:style>
	<g:VerticalPanel>
		<g:HTML ui:field="dialogText">Please select a document to open:</g:HTML>
		<g:FlexTable ui:field="availableDocumentsTable">
						
		</g:FlexTable>
		<g:FlowPanel addStyleNames="{style.buttonbar}">
			<g:Button ui:field="cancelButton">Cancel</g:Button>
			<g:Button ui:field="okButton">OK</g:Button>
		</g:FlowPanel>
	</g:VerticalPanel>
</ui:UiBinder> 